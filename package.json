{
  "name": "okos-gyakorlo",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:network": "vite --host 0.0.0.0",
    "dev:api": "node dev-server.js",
    "dev:simple": "node simple-server.js",
    "dev:drive": "node server.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:api\" \"npm run dev:simple\" \"npm run dev:drive\"",
    "dev:network-full": "concurrently \"npm run dev:network\" \"npm run dev:api\" \"npm run dev:simple\" \"npm run dev:drive\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "preview:network": "vite preview --host 0.0.0.0",
    "test": "node tests/test-runner.js",
    "test:security": "node tests/security.test.js",
    "test:game-flow": "node tests/game-flow.test.js",
    "test:network": "node test-network-session.js",
    "db:init": "node -r ts-node/register database/init.ts",
    "db:health": "node scripts/test-db.js",
    "supabase:setup": "node scripts/setup-supabase.js",
    "supabase:tables": "node scripts/create-tables.js",
    "supabase:install": "npm install @supabase/supabase-js",
    "vercel-build": "npm run build",
    "start": "node simple-server.js"
  },
  "dependencies": {
    "@google/genai": "*",
    "@supabase/supabase-js": "^2.90.1",
    "@types/pg": "^8.16.0",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@vercel/node": "^5.5.16",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "bcryptjs": "^3.0.3",
    "dotenv": "^17.2.4",
    "google-auth-library": "^10.5.0",
    "googleapis": "^171.4.0",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.344.0",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "concurrently": "^9.2.1",
    "cors": "^2.8.6",
    "express": "^5.2.1"
  }
